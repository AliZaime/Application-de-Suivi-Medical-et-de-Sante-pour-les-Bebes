{
  "expo": {
    "name": "baby-health-expo",
    "slug": "baby-health-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/baby-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "Cette application utilise les notifications pour rappeler des événements importants."
      },
      "bundleIdentifier": "com.esisa.babyhealth"
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS"
      ],
      "package": "com.esisa.babyhealth",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c2529b1b-602e-4b0f-be1e-e6c49bbb9a2e"
      }
    }
  }
}
